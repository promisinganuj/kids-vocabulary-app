# Azure OpenAI Configuration
# Copy this file to .env and fill in your actual values

# Your Azure OpenAI API Key
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here

# Azure OpenAI Endpoint (e.g., https://your-resource-name.openai.azure.com/)
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/

# Azure OpenAI Deployment Name (the name you gave to your deployed model)
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini

# Azure OpenAI API Version (e.g., 2024-02-15-preview)
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# Admin Configuration
# Default admin password (change this immediately after first login)
ADMIN_DEFAULT_PASSWORD=VocabAdmin2025!
